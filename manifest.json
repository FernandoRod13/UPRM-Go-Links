{
    "name": "go uprm",
    "version": "1.0",
    "omnibox": { "keyword" : "go" },
    "background": {
        "scripts": ["background.js", "credentials.js"],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "browser_action": {
        "default_popup": "popup.html"
